        "exitAfterOneJob": false
      },
      {
        "queue": "ci-group-6-idle",
        "name": "kb-cigroup-6-idle",
        "machineType": "n2-custom-6-16384",
        "localSsds": 1,
        "idleTimeoutMins": 10,
        "exitAfterOneJob": false
      }
    ]
  }
